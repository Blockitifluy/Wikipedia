{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["dom", "dom.iterable", "ES2022", "ES2022.Array", "ES2022.Object"],
    "allowJs": true,
    "importHelpers": false,
    "downlevelIteration": true,
    "skipLibCheck": false,
    "strict": false,
    "noEmit": false,
    "removeComments": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "./app/resources"
  },
  "include": ["**/*"]
}